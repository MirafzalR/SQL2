CREATE TABLE Employees (
    EmpID INT PRIMARY KEY,
    Name VARCHAR(50),
    Salary DECIMAL(10,2)
);

INSERT INTO Employeess (EmpID,Name,Salary)
VALUES (1, 'Alice', 5000.00);



INSERT INTO Employeess (EmpID, Name, Salary)
VALUES 
(2, 'Bob', 4500.00),
(3, 'Charlie', 6000.00);



UPDATE Employeess
SET Salary = 7000
WHERE EmpID = 1;



DELETE FROM Employeess
WHERE EmpID = 2;
